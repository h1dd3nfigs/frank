	<script> 
		try{document.domain="aliexpress.com";}catch(e){}
		var globalFrameHeight; 
		function ImgAuto(i,MaxW,MaxH){var o=new Image();o.src=i.src;var w=o.width;var h=o.height;var t;if (w>MaxW){t=MaxW;}else{t=w;}if ((h*t/w)>MaxH){i.height=MaxH;i.width=MaxH/h*w;}else{i.width=t;i.height=t/w*h;}}		
		function changeHeight(vheight){ 
	
			if(!this.parent.document.getElementById('feedback')){ 
				globalFrame = this.parent.document.getElementById('feedback');
			} else{ 
				globalFrame = this.parent.document.getElementById('feedback').getElementsByTagName('iframe')[0];		
			}
			
			if(!-[1,]) {
				// if broswer is ie
				if(vheight<0){ 
					globalFrame.height = globalFrame.getAttribute('sbheight');
				}else{
					globalFrame.height = parseInt(globalFrame.height) + vheight;
				}
			}
			else {
				if(vheight<0){ 
					globalFrame.height = globalFrame.getAttribute('sbheight');
				}else{
				// if broswer is not ie
					globalFrame.height = parseInt(globalFrame.height) + vheight;
				}
			}

			
		}	
		seajs.use('$',function(J){ 				
		
			J(document.body).click(function(event){ 
				if($(event).target.className!='zoom'){
					J('.biPic img').remove();
					J('.outpic').removeClass('open');
					J('.smpic .pic-mark').removeClass('bd');
					J('#transction-feedback').removeClass('addHeight');
					changeHeight(-400);
					
				}				

			})
			J('.smpic .pic-mark').click(function(event){
				//console.log($(event).target);
				J('.outpic').find('.biPic').html('');
				J('.outpic').removeClass('open');
				if(J('#transction-feedback').hasClass('addHeight')){ 
					changeHeight(-400);
					J('#transction-feedback').removeClass('addHeight');
				}
				var parent=J(this).closest('.outpic');
				if(!parent.hasClass('open')){ 
					parent.addClass('open');
					changeHeight(400);	
					J('#transction-feedback').addClass('addHeight');
				} 
				var curImg=J(this).find('img');
				var imgSrc=curImg.attr('src');
				J('.smpic .pic-mark').removeClass('bd');
				J(this).addClass('bd');
				J(this).closest('.outpic').find('.biPic').html("<a href='"+imgSrc+"' target='_blank'><img src='"+imgSrc+"' height='400' onload='ImgAuto(this,300,400)'/></a>");
							
			});
			J('.pic-mark').hover(
			  function () {
				J(this).find('.zoom').show();
			  },
			  function () {
				J(this).find('.zoom').hide().removeClass('hover');
			  }
			);
			
		})	
		
	</script>
